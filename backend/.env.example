# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=bankingappdb
DB_USER=root
DB_PASS=your_database_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_complex
JWT_REFRESH_SECRET=your_refresh_secret_key_here
JWT_EXPIRE=15m
JWT_REFRESH_EXPIRE=7d

# Email Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_gmail_app_password

# Google reCAPTCHA
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key

# Partner Bank Security
RSA_PRIVATE_KEY=your_rsa_private_key
RSA_PUBLIC_KEY=your_rsa_public_key
PARTNER_SECRET_KEY=your_partner_secret_key

# API Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
